{
      "version": 2,
      "builds": [
        {
          "src": "./index.js",
          "use": "@vercel/node"
        }
      ],
	"routes": [
		{
		  "src": "/loginUser",
		  "dest": "/loginUser",
		  "methods": ["POST"],
		  "headers": {
		    "Access-Control-Allow-Origin": "http://localhost:3000"
		  }
		},
		{
		  "src": "/(.*)",
		  "dest": "/",
		  "methods": ["GET", "POST", "PUT", "DELETE", "OPTION"]
		}
	]
  }